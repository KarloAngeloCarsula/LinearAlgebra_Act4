    Vector1 = [(1,2,3,4)]
    Vector2 = [(2,4,6,8)]
    Vector3 = [(3,6,9,12)]
    Vector4 = [(4,8,12,16)]
    Vector5 = [(5,10,15,20)]
    Vector6 = [(6,12,18,24)]

V1 =[]
V2 =[]
V3 =[]
V4 =[]
V5 =[]
V6 =[]

for Elements in Vector1:
    for numbers in Elements:
        sqrt = numbers ** 2
        V1.append(sqrt)

for Elements in Vector2:
    for numbers in Elements:
        sqrt = numbers ** 2
        V2.append(sqrt)

for Elements in Vector3:
    for numbers in Elements:
        sqrt = numbers ** 2
        V3.append(sqrt)

for Elements in Vector4:
    for numbers in Elements:
        sqrt = numbers ** 2
        V4.append(sqrt)

for Elements in Vector5:
    for numbers in Elements:
        sqrt = numbers ** 2
        V5.append(sqrt)

for Elements in Vector6:
    for numbers in Elements:
        sqrt = numbers ** 2
        V6.append(sqrt)

print('Vector 1 =', Vector1)
print('Squared of All Number in Vector 1 =', V1)
print('Sum of of All Number in Vector 1 =', sum(V1))
print('Square root of Vector 1 =', sum(V1) ** (1/2))
print(" " * 66)
print('∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼')
print('Vector 2 =', Vector2)
