print('Sum of of All Number in Vector 2 =', sum(V2))
print('Square root of Vector 2 =', sum(V2) ** (1/2))
print(" " * 66)
print('∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼∼')
print('Vector 3 =', Vector3)
print('Squared of All Numbers in Vector 3 =', V3)
print('Sum of of All Number in Vector 3 =', sum(V3))
print('Square root of Vector 3 =', sum(V3) ** (1/2))
